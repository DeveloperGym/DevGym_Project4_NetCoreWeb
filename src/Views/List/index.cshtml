@model List<src.Models.AList>
@{
    ViewData["Title"] = "All Lists";
}
<h2>@ViewData["Title"]</h2>
<br />
<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>Title</th>
                <th>-</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var OneItem in Model)
            {
            <tr>
                <td>@OneItem.Title</td>
                <td><a href='@Url.Action("Show", "List", new { id = OneItem.ID })'>View</a></td>
            </tr>
            }
        </tbody>
        <tfoot>
            <tr>
                <td colspan="2">List Count: @Model.Count</td>
            </tr>
        </tfoot>
    </table>
</div>